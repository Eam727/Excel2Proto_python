{
  "version": "1.0.0-*",
  "description": "Tests.Dnx Class Library",
  "authors": [ "marc" ],
  "buildOptions": {
    "define": [ "XUNITBRIDGE", "XUNIT", "NO_INTERNAL_CONTEXT", "NO_NHIBERNATE", "FEAT_COMPILER", "NO_ENYIM", "NO_CODEGEN", "NO_WCF", "IMPORT_TESTS" ],
    "strongName": true,
    "keyFile": "../ProtoBuf.snk",
    "compile": {
      "include": [
        "../FX11/*.cs",
        "../protobuf-net.unittest/**/*.cs",
        "../SampleDTO/*.cs",
        "../Examples/**/*.cs"
      ],
      "exclude": [
        "*AssemblyInfo.cs",
        "Settings.Designer.cs"
      ]
    },
    "copyToOutput": [ "../Tools/nwind.proto.bin" ]
  },
  "dependencies": {
    "protobuf-net": {
      "target": "project",
      "version": "2.1.0-alpha-5"
    },
    "xunit": "2.1.0",
    "dotnet-test-xunit": "1.0.0-*"
  },
  "testRunner": "xunit",
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Data.Linq": "4.0.0.0",
        "System.ServiceModel": "4.0.0.0",
        "System.Drawing": "4.0.0.0",
        "System.Data": "4.0.0.0",
        "System.Web.Extensions": "4.0.0.0",
        "PresentationCore": "4.0.0.0",
        "WindowsBase": "4.0.0.0"
      }
    },
    "netcoreapp1.0": {
      "imports": [ "dnxcore50", "portable-net45+win8" ],
      "buildOptions": {
        "define": [ "COREFX" ]
      },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        },
        "System.Collections": "4.0.11",
        "System.Collections.NonGeneric": "4.0.1",
        "System.Linq": "4.1.0",
        "System.Runtime": "4.1.0",
        "System.Threading": "4.0.11",
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "System.Runtime.Serialization.Xml": "4.1.1",
        "System.Data.SqlClient": "4.1.0",
        "System.Data.Common": "4.1.0",
        "System.Diagnostics.TraceSource": "4.0.0",
        "System.Collections.Immutable": "1.2.0",
        "System.ComponentModel": "4.0.1",
        "System.Threading.Tasks.Parallel": "4.0.1"
      }
    }
  }
}
